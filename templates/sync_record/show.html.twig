{% extends 'base-with-menu.html.twig' %}

{% block title %}SyncRecord{% endblock %}

{% block body %}
    <h1>SyncRecord</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sync_record.id }}</td>
            </tr>
            <tr>
                <th>SmartAccountsApiKeyPublic</th>
                <td>{{ sync_record.smartAccountsApiKeyPublic }}</td>
            </tr>
            <tr>
                <th>SmartAccountsApiKeyPrivate</th>
                <td>{{ sync_record.smartAccountsApiKeyPrivate }}</td>
            </tr>
            <tr>
                <th>TransferWiseApiToken</th>
                <td>{{ sync_record.transferWiseApiToken }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ sync_record.name }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ sync_record.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ sync_record.created ? sync_record.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ sync_record.updated ? sync_record.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sync_record_index') }}">back to list</a>

    <a href="{{ path('sync_record_edit', {'id': sync_record.id}) }}">edit</a>

    {{ include('sync_record/_delete_form.html.twig') }}
{% endblock %}
